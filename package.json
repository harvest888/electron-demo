{
  "name": "electron-quick-start",
  "version": "1.0.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron --inspect=5859 .",
    "pao": "nodemon --watch main.js --exec \"electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "cross-env GH_TOKEN=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 electron-builder"
  },
  "repository": "https://github.com/harvest888/electron-demo",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "devtron": "^1.4.0",
    "electron": "18.2.2",
    "electron-builder": "^22.13.1",
    "nodemon": "^2.0.14"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1"
  },
  "build": {
    "appId": "your.id",
    "productName": "OMS",
    "compression": "store",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "./sad.ico"
    },
    "publish": ["github"],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./sad.ico",
      "uninstallerIcon": "./sad.ico",
      "installerHeaderIcon": "./sad.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "xxxx",
      "include": "./installer.nsh"
    }
  }
}
